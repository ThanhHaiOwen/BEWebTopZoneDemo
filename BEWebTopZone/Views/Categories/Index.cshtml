@model IEnumerable<BEWebTopZone.Models.Category>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Quản Lý Loại Thiết Bị</h2>


<table class="table">
    <tr>
        <th>
            Tên Loại Thiết Bị
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameCate)
            </td>
            <td>
                @Html.ActionLink("Sửa", "Edit", new { id = item.CatID }) |
                @Html.ActionLink("Chi Tiết", "Details", new { id = item.CatID }) |
                @Html.ActionLink("Xóa", "Delete", new { id = item.CatID })
            </td>
        </tr>
    }
    </table>
<p>
    @Html.ActionLink("Tạo nới", "Create")
</p>